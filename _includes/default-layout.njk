<head>
  <meta charset="utf-8">
  <title>{{ html_title }}</title>
  <meta name="author" content="technikum29-Team">
  <meta name="generator" content="t29v8/{{ eleventy.generator }}">
  <meta name="t29.cachedate" content="{{ html_time }}">
  <meta name="t29.page_id" content="{{ page_id }}">
  {% for header in header_prepend %}
  {{ header | safe }}
  {% endfor %}
  
  <!-- t29v6 template.php has a ton of other stuff here, amongst others:
       logic for passing data to client side JS, apple logos,
       links for RSS, interlanguage, JS, CSS -->
       
  <meta name="viewport" content="width=device-width,initial-scale=1">
</head>
<body>

<!-- navigation start -->

<!-- Show the tour, without the tour link itself -->
{% set navPages = collections.all | eleventyNavigation("tour") %}
  
{% macro renderNavListItem(entry, level) -%}

<li class="{% if entry.url == page.url %}active{% endif %} {{ entry.data.nav_class }}">
  <a href="{{ entry.url }}" data-id="{{ entry.data.page_id }}" data-key="{{ entry.key }}" title="{{ entry.data.title }}">{{ entry.title }}</a>
{%- if entry.children.length -%}
  <ul class="u{{level}}">
    {%- for child in entry.children %}{{ renderNavListItem(child, level+1) }}{% endfor -%}
  </ul>
{%- endif -%}
</li>
{%- endmacro %}

<ul class="u1">
{%- for entry in navPages %}{{ renderNavListItem(entry, 2) }}{%- endfor -%}
</ul>

<!-- navigation end -->

<!-- Begin of content -->
<!--{ content | safe }-->
<!-- end of content -->

</body>
</html>
